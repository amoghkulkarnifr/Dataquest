k-word;\n  overflow-x: auto;\n}\n\n$standard_transition: all ease 0.25s;\n\n%button {\n  display: inline-flex;\n  flex-direction: row;\n  align-content: center;\n  align-items: center;\n  justify-content: center;\n  text-align: center;\n  text-decoration: none !important;\n  white-space: nowrap;\n  line-height: 1em;\n  transition: $standard_transition;\n\n  color: $color_white;\n  background-color: transparent;\n  border-color: transparent;\n\n  &:hover {color: $color_white;}\n  &:focus {outline: none;}\n  &.disabled, &.disabled:hover {\n    cursor: default;\n    color: lighten($color_disabled_text, 20%);\n  }\n}\n\n%button_as_text_on_dark {\n  @extend %meta_font;\n  cursor: pointer;\n  color: $color_dark_bkg_purple;\n  &:hover {color: $color_dark_bkg_green;}\n  transition: $standard_transition;\n}\n%button_as_text_on_light {\n  @extend %meta_font;\n  cursor: pointer;\n  color: $color_black;\n  &:hover {color: $color_light_bkg_green;}\n  transition: $standard_transition;\n}\n\n%panel-box {\n  padding: 8px 20px;\n  border-radius: 12px;\n  border-width: 2px;\n  border-style: solid;\n  background-color: transparent;\n}\n%drop_down_on_toolbar {\n  @extend %button;\n  @extend %panel-box;\n  border-radius: 0;\n  line-height: 1em;\n  color: $color_light_bkg_purple;\n  border-color: $color_light_bkg_purple;\n  cursor: pointer;\n  &:focus {\n    outline: none;\n  }\n  &:hover {\n    color: $color_light_bkg_purple;\n  }\n}\n\n%vertical-center {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n%hanging-indent {\n  padding-left: 2ch;\n  text-indent: -2ch;\n}\n\n%dashboard_title {\n  @extend %heading_font;\n  @extend %hanging-indent;\n  text-transform: capitalize;\n  &, a {\n    color: $color_light_bkg_purple;\n    transition: $standard_transition;\n    text-decoration: none;\n  }\n  a:hover {\n    color: $color_light_bkg_green;\n  }\n}\n\n%dashboard_description {\n  @extend %meta_font;\n  color: $color_light_bkg_purple;\n}\n\n%scroller {\n  position: ab